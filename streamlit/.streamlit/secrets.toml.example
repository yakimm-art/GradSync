# Copy this to secrets.toml (don't commit secrets.toml!)
# Or paste into Streamlit Cloud Secrets tab

[snowflake]
account = "your-account.region"  # e.g., "abc12345.us-east-1"
user = "your_username"
password = "your_password"
warehouse = "GRADSYNC_WH"
database = "GRADSYNC_DB"
schema = "RAW_DATA"
role = "PUBLIC"
